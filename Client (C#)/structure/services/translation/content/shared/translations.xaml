            <system:String x:Key="staff">طاقم العمل</system:String>
        </ResourceDictionary>
        <ResourceDictionary x:Key="English">
            <system:String x:Key="school_owner">School Owner</system:String>
            <system:String x:Key="headmaster">Headmaster</system:String>
            <system:String x:Key="teacher_supervisor">Teacher Supervisor</system:String>
